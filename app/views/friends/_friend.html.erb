<tbody id="<%= dom_id friend %>">
<tr>
<td>    <%= friend.first_name %> </td>
<td>    <%= friend.last_name %> </td>
<td>    <%= friend.email %> </td>
<td>    <%= friend.phone %> </td>
<td>    <%= friend.twitter %> 
<td><%= link_to "Show", friend, class:'btn btn-primary' %> 
<%= link_to "Edit", edit_friend_path(friend), class:'btn btn-primary' %>
 <%= link_to "Destroy", friend, method: :delete, data: { confirm: "Are you sure?" }, class:'btn btn-primary' %> </td>
</tr>
</tbody>
